                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Cut" />
                  <MenuItem mnemonicParsing="false" text="Copy" />
                  <MenuItem mnemonicParsing="false" text="Paste" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Select All" />
              <MenuItem mnemonicParsing="false" text="Delete" />            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Project">
          	<items>
                  <MenuItem mnemonicParsing="false" text="Open Project" />
                  <MenuItem mnemonicParsing="false" text="Close Project" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Properties" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Window">
              <items>
                <MenuItem mnemonicParsing="false" text="New Window" />
                  <Menu mnemonicParsing="false" text="Appearances">
                    <items>
                      <MenuItem mnemonicParsing="false" text="Show Toolbar" />
                        <MenuItem mnemonicParsing="false" text="Toogle Fullscreen" />
                    </items>
                  </Menu>
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <Menu mnemonicParsing="false" text="Perspective">
                    <items>
                      <MenuItem mnemonicParsing="false" text="2D" />
                        <MenuItem mnemonicParsing="false" text="3D" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Save Perspective" />
                        <MenuItem mnemonicParsing="false" text="Reset Perspective" />
                        <MenuItem mnemonicParsing="false" text="Close Perspective" />
                    </items>
                  </Menu>
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Preferences" />
              </items>
            </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
<MenuItem mnemonicParsing="false" text="Welcome" />
                  <MenuItem mnemonicParsing="false" text="Search" />
              <MenuItem mnemonicParsing="false" text="About" />            
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <left>
      <VBox prefHeight="675.0" prefWidth="170.0" BorderPane.alignment="CENTER">
         <children>
            <ComboBox fx:id="containerBox" onAction="#handleContainer" prefWidth="170.0" />
            <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="170.0" text="Draw" />
            <Button mnemonicParsing="false" prefWidth="170.0" text="Dispense" />
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <ListView fx:id="listView" prefHeight="596.0" prefWidth="170.0" />
               </children>
            </AnchorPane>
         </children>
      </VBox>
   </left>
   <center>
      <Canvas fx:id="myCanvas" height="670.0" width="530.0" BorderPane.alignment="TOP_LEFT" />
   </center>
</BorderPane>
