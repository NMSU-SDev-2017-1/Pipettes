<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="root" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pipettes.ui.preview.PreviewPresenter">
	<children>
		<ToolBar>
			<items>
				<Button mnemonicParsing="false" onAction="#onReset" text="Reset" />
				<Pane maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
				<HBox id="playControls" prefHeight="-1.0" prefWidth="-1.0">
					<children>
						<Button fx:id="startButton" mnemonicParsing="false" styleClass="left-pill" text="|&lt;">
							<graphic>
								<Pane prefHeight="16.0" prefWidth="16.0" />
							</graphic>
						</Button>
						<Button fx:id="rewindButton" mnemonicParsing="false" styleClass="center-pill" text="&lt;&lt;">
							<graphic>
								<Pane prefHeight="16.0" prefWidth="16.0" />
							</graphic>
						</Button>
						<ToggleButton fx:id="playButton" mnemonicParsing="false" styleClass="center-pill" text="&gt;">
							<graphic>
								<Pane prefHeight="16.0" prefWidth="16.0" />
							</graphic>
						</ToggleButton>
						<Button fx:id="fastForwardButton" mnemonicParsing="false" styleClass="center-pill" text="&gt;&gt;">
							<graphic>
								<Pane prefHeight="16.0" prefWidth="16.0" />
							</graphic>
						</Button>
						<Button fx:id="endButton" mnemonicParsing="false" styleClass="center-pill" text="&gt;|">
							<graphic>
								<Pane prefHeight="16.0" prefWidth="16.0" />
							</graphic>
						</Button>
						<ToggleButton fx:id="loopButton" mnemonicParsing="false" selected="true" styleClass="right-pill" text="R">
							<graphic>
								<Pane prefHeight="16.0" prefWidth="16.0" />
							</graphic>
						</ToggleButton>
					</children>
				</HBox>
			</items>
		</ToolBar>
		<AnchorPane fx:id="viewPane" focusTraversable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" style="-fx-background-color: radial-gradient(center 50% 50%, radius 75%, rgba(92,92,92,0.9) 0%, rgba(32,32,32,0.9) 100%);" VBox.vgrow="ALWAYS" />
	</children>
</VBox>
